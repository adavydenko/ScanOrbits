function dy = myODE_full(t, y, varargin)
    gamma = varargin{1}(1);
    r0 = varargin{1}(2);

    dy = zeros(4, 1);
    
%     F1 = -(1+(y(1)^2+y(2)^2)*r0^2)^(-3/2)*y(1)*r0^2;
%     F2 = -(1+(y(1)^2+y(2)^2)*r0^2)^(-3/2)*y(2)*r0^2;
    
    F1 = -(1+(y(1)^2+y(2)^2)*r0^2)^(-3/2)*y(1)*r0^2;
    F2 = -(1+(y(1)^2+y(2)^2)*r0^2)^(-3/2)*y(2)*r0^2;
    
    dy(1) = y(3);
    dy(2) = y(4);
    dy(3) = +gamma * y(4) + F1 + y(1);
    dy(4) = -gamma * y(3) + F2;