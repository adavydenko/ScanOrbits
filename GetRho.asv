function Rho = GetRho (E, h, r0)
    
    dRho = 0.1    
    Rho(1) = 0
    
    r_max = 1
    
    r = 0
    while ((r+dRho) <= r_max)
        if sqrVr(E, h, (r+dRho), r0) >= 0
        t = t+dRho
        Rho = [Rho t]
    end
    
    r(1) = []%Удаляем 0
    Rho = r
        
    function result = sqrVr(E, h, r, r0)
        result = (2*E + 2*F(r, r0) - h^2/r^2)

    function result = F(r, r0)
        result = (1+r0^2*r^2)^(-1/2)
            
        